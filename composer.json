{
  "name": "spryker-shop/shop-layout-extension",
  "description": "ShopLayoutExtension module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*",
    "spryker/symfony": "*"
  },
  "suggest": {
    "spryker/symfony": "3.0.0 for using LanguageSwitcherPluginInterface"
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerShopTest\\": "tests/SprykerShopTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
